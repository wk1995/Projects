<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wk.period_record">

    <application
        android:name="com.wk.period.module.PeriodRecordApp"
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/module_icon"
        android:label="@string/module_name"
        android:theme="@style/Theme.AppCompat"
        tools:replace="android:label">
        <activity android:name="com.wk.period.PeriodRecordMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>